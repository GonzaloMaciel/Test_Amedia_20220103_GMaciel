            @Html.DisplayNameFor(model => model.NroDoc)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.NroDoc)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.CodRol)
        </dt>
        <dd class="col-sm-9">
