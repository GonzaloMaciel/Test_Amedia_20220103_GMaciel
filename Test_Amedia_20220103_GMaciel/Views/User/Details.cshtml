@model Test_Amedia_20220103_GMaciel.Models.TUser
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container p-3 my-3 border">
    <h1>User Details</h1>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.CodUsuario)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.CodUsuario)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.TxtUser)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.TxtUser)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.TxtPassword)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.TxtPassword)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.TxtNombre)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.TxtNombre)
        </dd>
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.TxtApellido)
        </dt>
