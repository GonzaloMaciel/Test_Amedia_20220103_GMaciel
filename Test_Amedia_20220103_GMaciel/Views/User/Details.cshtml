    </dl>
    <div>
        <a class="btn btn-primary" asp-action="AddOrEdit" asp-route-codUsuario="@Model.CodUsuario">Edit</a>
        <a class="btn btn-danger" asp-action="Index">Back</a>
    </div>
</div>
